{
	"eslint.packageManager": ["yarn", "npm"],
	"eslint.autoFixOnSave": true,
	"eslint.codeAction.showDocumentation": {
		"enable": true
	},
	"eslint.probe": [
		"javascript",
		"javascriptreact",
		"typescript",
		"typescriptreact",
		"html",
		"svelte",
		"markdown"
	],
	"eslint.enable": true,
	"eslint.run": "onSave",
	"prettier.tabWith": 4,
	"prettier.useTabs": true,
	"diagnostic.errorSign": "üî•",
	"diagnostic.warningSign": "‚ö†Ô∏è",
	"diagnostic.infoSign": "‚ÑπÔ∏è",
	"diagnostic.checkCurrentLine": true,
	"coc.preferences.formatOnSaveFiletypes": [
		"javascript",
		"typescript",
		"javascriptreact",
		"typescriptreact",
		"typescript.tsx",
		"svelte",
		"html",
		"json"
	],
	"languageserver": {
		"golang": {
			"command": "gopls",
			"rootPatterns": ["go.mod"],
			"filetypes": ["go"]
		}
	},
	"list.sources.sumbols.excludes": [
		"**/node_modules/**",
		"**/node_modules/**/.*",
		"**/node_modules/**/.*/**"
	],
	"svelte.enable-ts-plugin": true
}
